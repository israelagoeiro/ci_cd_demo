# Configurações de ambiente para CI/CD Demo - EXEMPLO
# Copie este arquivo para .env e preencha com seus valores

# GitHub
GITHUB_TOKEN=seu_token_aqui
GITHUB_OWNER=seu_usuario_github
GITHUB_REPO=nome_do_repositorio

# Docker Hub
DOCKERHUB_USERNAME=seu_usuario_dockerhub
DOCKERHUB_TOKEN=seu_token_dockerhub

# Configurações da API
API_URL=http://localhost:8080
API_KEY=chave_secreta_api

# Configurações do Banco de Dados (se necessário no futuro)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nome_do_banco
DB_USER=usuario_db
DB_PASSWORD=senha_db 